function calculateGrade(marks) {
    const userInput = prompt("Enter the student's marks(0-100)")

    if (marks >=80 && marks <=100) {
     return "A";
    } else if(marks >=60 && marks < 80) {
     return"B";
    } else if (marks >= 50 && marks < 60) {
     return "C";
    } else if (marks >= 40 && marks < 50) {
     return "D";
    } else {
     return "E";
    }
    
}
 

